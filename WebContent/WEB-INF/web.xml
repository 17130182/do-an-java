<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
   id="WebApp_ID" version="3.0">
   <display-name>Website Nông Sản</display-name>
      
   <servlet>
       <servlet-name>addCategory</servlet-name>
       <servlet-class>nongsan.webmvc.controller.admin.CategoryAddController</servlet-class>
   </servlet>
 
   <servlet-mapping>
       <servlet-name>addCategory</servlet-name>
       <url-pattern>/admin/cate/add</url-pattern>
   </servlet-mapping>
   
   <servlet>
       <servlet-name>deleteCategory</servlet-name>
       <servlet-class>nongsan.webmvc.controller.admin.CategoryeDeleteController</servlet-class>
   </servlet>
   
   <servlet-mapping>
       <servlet-name>deleteCategory</servlet-name>
       <url-pattern>/admin/cate/delete</url-pattern>
   </servlet-mapping>
   
   
   <servlet>
       <servlet-name>listCategory</servlet-name>
       <servlet-class>nongsan.webmvc.controller.admin.CategoryListController</servlet-class>
   </servlet>
 
   <servlet-mapping>
       <servlet-name>listCategory</servlet-name>
       <url-pattern>/admin/cate/list</url-pattern>
   </servlet-mapping>
 
</web-app>
